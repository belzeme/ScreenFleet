{
    "name": "screen-fleet",
    "version": "0.0.0",
    "description": "screen-fleet",
    "license": "MIT",
    "repository": "",
    "author": "",
    "keywords": [
	""
    ],
    "files": [
	"dist"
    ],
    "main": "dist/server.js",
    "typings": "dist/index.d.ts",
    "scripts": {
	"clean": "rimraf dist",
	"copyconf": "mkdir -p dist/config && cp src/config/*.json dist/config",
	"format": "prettier --write \"{src,test}/**/*.ts\"",
	"lint": "tslint --force --format verbose \"src/**/*.ts\"",
	"prepublishOnly": "npm run build",
	"build": "yarn clean && yarn copyconf && yarn lint && echo Using TypeScript && tsc --version && tsc --pretty",
	"test": "npm run build && mocha --compilers ts:ts-node/register --recursive \"test/**/*-spec.ts\"",
	"coverage": "nyc --include=\"src/**/*.ts\" --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive \"test/**/*-spec.ts\"",
	"watch": "npm run build -- --watch",
	"watch:test": "npm run test -- --watch",
	"start": "yarn build && node dist/src/server.js"
    },
    "dependencies": {
	"express": "^4.16.3",
	"morgan": "^1.9.0",
	"winston": "^2.4.2"
    },
    "devDependencies": {
	"@types/chai": "^3.0.0",
	"@types/express": "^4.11.1",
	"@types/mocha": "^2.0.0",
	"@types/morgan": "^1.7.35",
	"@types/node": "^8.0.0",
	"@types/winston": "^2.3.9",
	"chai": "^3.0.0",
	"coveralls": "^2.0.0",
	"mocha": "^3.0.0",
	"nyc": "^10.0.0",
	"prettier": "^1.5.2",
	"rimraf": "^2.0.0",
	"ts-loader": "^4.2.0",
	"ts-node": "^3.0.0",
	"tslint": "^5.0.0",
	"tslint-config-prettier": "^1.1.0",
	"typescript": "^2.3.0"
    },
    "engines": {
	"node": ">=6.0.0"
    },
    "nyc": {
	"include": [
	    "src/**/*.ts"
	],
	"exclude": [
	    "dist"
	],
	"extension": [
	    ".ts"
	],
	"require": [
	    "ts-node/register"
	],
	"reporter": [],
	"all": true
    }
}
